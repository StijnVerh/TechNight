	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"name": "Serve Frontend",
			"request": "launch",
			"runtimeExecutable": "npm",
			"runtimeArgs": ["run"],
			"args": ["start"],
			"cwd": "${workspaceFolder}/app/smarttable",
			"restart": true,
			"console": "integratedTerminal"
		},
		{
			"type": "node",
			"name": "Start Server",
			"request": "launch",
			"runtimeExecutable": "npm",
			"runtimeArgs": ["run"],
			"args": ["start"],
			"cwd": "${workspaceFolder}",
			"restart": true,
			"console": "integratedTerminal"
		},
		{
			"type": "node",
			"name": "Start Server Hybrid",
			"request": "launch",
			"runtimeExecutable": "npm",
			"runtimeArgs": ["run"],
			"args": ["start:hybrid"],
			"cwd": "${workspaceFolder}",
			"restart": true,
			"console": "integratedTerminal"
		}
	]
}
